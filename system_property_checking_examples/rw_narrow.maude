mod R&W is
  sorts Nat Config .
  op <_,_> : Nat Nat -> Config [ctor] .
  op $ : -> [Config] .     *** unreachable state
  op 0 : -> Nat [ctor] .
  op s : Nat -> Nat [ctor] .

  vars R W N M I J : Nat .

  rl < 0, 0 > => < 0, s(0) > [narrowing] .
  rl < R, s(W) > => < R, W > [narrowing] .
  rl < R, 0 > => < s(R), 0 > [narrowing] .
  rl < s(R), W > => < R, W > [narrowing] .
endm
